	"version": "0.2.0",
	"configurations": [
		{
			"type": "chrome",
			"request": "launch",
			"name": "Launch Chrome against local IP",
			"url": "http://192.168.1.14:1234",
			"webRoot": "${workspaceFolder}",
			"breakOnLoad": true,
			"sourceMapPathOverrides": {
				"/__parcel_source_root/*": "${webRoot}/*"
				// "/source/*": "${webRoot}/*"
			}
		}
	]
}
